public class EmployeeRunner {
  public static void main(String[] args){
    JavaApplication1 e1 = new JavaApplication1("Rey", "Zophia","202013","November 3, 2003", 4500);
    HourlyEmployee e2 = new HourlyEmployee("Ganda","ko","201221","February 3, 2011", 500);
    ContractEmployee e3 = new ContractEmployee("Kim","Jiwoong","203333","February 5, 2011", 200000);
    
    e1.displayRateOfPay();
    e2.displayRateOfPay();
    e3.displayRateOfPay();
  }
}
